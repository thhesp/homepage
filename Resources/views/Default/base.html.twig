{# app/Resources/views/Default/base.html.twig #}
<html>
    <head>
        {# ... #}
        <meta name="viewport" content="width=device-width, initial-scale=1">

        {% block stylesheets %}
            <link href="{{ asset('bundles/tshomepage/css/bootstrap.min.css') }}" rel="stylesheet" />

            <link href="{{ asset('bundles/tshomepage/css/main.css') }}" rel="stylesheet" />
        {% endblock %}
    </head>
    <body>
        <div class="container-fluid">
            {% block header %}
                {{ include('TSHomepageBundle:Elements:header.html.twig') }}


                {{ include('TSHomepageBundle:Elements:navigation.html.twig') }}
            {% endblock %}

            {% block body %}

            {% endblock %}

            {% block footer %}
                {{ include('TSHomepageBundle:Elements:footer.html.twig') }}
            {% endblock %}
        </div>
        {% block javascripts %}

            <script src="{{ asset('bundles/tshomepage/js/lib/jquery-2.1.3.min.js') }}"></script>
            <script src="{{ asset('bundles/tshomepage/js/lib/bootstrap.min.js') }}"></script>
        {% endblock %}
    </body>
</html>